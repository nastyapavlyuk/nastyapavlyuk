#include <iostream>
#include <cmath>
using namespace std;

int Fibonach(int n) 
{
	if (n == 0)
		return 0;
	if (n == 1 || n == 2)
		return 1;
	if (n % 2 == 0) 
	{
		n /= 2;
		return pow(Fibonach(n + 1), 2) - pow(Fibonach(n - 1), 2);
	}
	else 
	{
		--n;
		n /= 2;
		return pow(Fibonach(n), 2) + pow(Fibonach(n + 1), 2);
	}
}

int main() 
{
	int n;
	cin >> n;
	cout << Fibonach(n);
	return 0;
}
